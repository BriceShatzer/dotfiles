[hooks]
#check if commit message is properly formatted (ie: starts with the ticket id)
pretxncommit.ticket_id_required = hg log -r $HG_NODE --template {desc} | grep -q "^[[:alpha:]]*-[[:digit:]]*[[:space:]]"

